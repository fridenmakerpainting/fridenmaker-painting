# Fridenmaker Painting - Environment Variables
# Copy this file to .env and fill in the values

# ===========================================
# CONTACT FORM
# ===========================================

# Form submission endpoint (Formspree, custom API, etc.)
VITE_FORM_API_ENDPOINT=

# Email notifications recipient
VITE_CONTACT_EMAIL=info@fridenmakerpainting.com

# ===========================================
# ANALYTICS & TRACKING
# ===========================================

# Google Analytics 4 Measurement ID
VITE_GA_TRACKING_ID=

# Google Tag Manager ID
VITE_GTM_ID=

# Facebook Pixel ID (optional)
VITE_FB_PIXEL_ID=

# ===========================================
# MAPS & LOCATION
# ===========================================

# Google Maps API Key (for embedded maps)
VITE_GOOGLE_MAPS_API_KEY=

# ===========================================
# CMS / CONTENT (Optional)
# ===========================================

# Contentful Space ID (if using Contentful CMS)
VITE_CONTENTFUL_SPACE_ID=

# Contentful Access Token
VITE_CONTENTFUL_ACCESS_TOKEN=

# ===========================================
# THIRD-PARTY INTEGRATIONS
# ===========================================

# Calendly scheduling link
VITE_CALENDLY_URL=

# Social Media Links
VITE_INSTAGRAM_URL=
VITE_FACEBOOK_URL=
VITE_HOUZZ_URL=
